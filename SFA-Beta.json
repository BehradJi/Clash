{
  "log": {
    "disabled": true
  },
  "dns": {
    "servers": [
      {
        "type": "tls",
        "tag": "dns-remote",
        "detour": "Proxy",
        "server": "8.8.8.8"
      },
      {
        "type": "local",
        "tag": "dns-local"
      },
      {
        "type": "udp",
        "tag": "dns-direct",
        "server": "8.8.8.8"
      }
    ],
    "rules": [
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-ir"
          },
          {
            "rule_set": "geoip-ir"
          }
        ],
        "server": "dns-local"
      }
    ],
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "tun",
      "address": "172.19.0.1/28",
      "auto_route": true
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "Proxy",
      "outbounds": [
        "Auto",
        "{all}"
      ]
    },
    {
      "type": "urltest",
      "tag": "Auto",
      "outbounds": [
        "{all}"
      ]
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-ir",
        "outbound": "direct"
      },
      {
        "rule_set": "geoip-ir",
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geosite-ir",
        "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geosite-ir.srs"
      },
      {
        "type": "remote",
        "tag": "geoip-ir",
        "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geoip-ir.srs"
      }
    ],
    "final": "Proxy",
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "default_domain_resolver": "dns-direct"
  }
}

