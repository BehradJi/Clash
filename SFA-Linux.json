{
  "log": {
    "disabled": true
  },
  "dns": {
    "servers": [
      {
        "type": "tls",
        "tag": "dns-remote",
        "detour": "Proxy",
        "server": "8.8.8.8"
      },
      {
        "type": "udp",
        "tag": "dns-direct",
        "server": "8.8.8.8"
      }
    ],
    "rules": [
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-ir"
          },
          {
            "rule_set": "geoip-ir"
          }
        ],
        "server": "dns-direct"
      }
    ],
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 7890
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "Proxy",
      "outbounds": [
        "auto",
        "{all}"
      ]
    },
    {
      "type": "urltest",
      "tag": "auto",
      "outbounds": [
        "{all}"
      ]
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-ir",
        "outbound": "direct"
      },
      {
        "rule_set": "geoip-ir",
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geosite-ir",
        "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geosite-ir.srs",
        "download_detour": "direct"
      },
      {
        "type": "remote",
        "tag": "geoip-ir",
        "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geoip-ir.srs",
        "download_detour": "direct"
      }
    ],
    "final": "Proxy",
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "dns-direct",
      "strategy": "prefer_ipv4",
      "rewrite_ttl": 60
    }
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090"
    }
  }
}


