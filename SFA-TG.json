{
  "log": {
    "disabled": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns-remote",
        "address": "tls://8.8.8.8"
      },
      {
        "tag": "dns-direct",
        "address": "5.200.200.200",
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns-direct"
      },
      {
        "rule_set": [
          "geoip-ir",
          "geosite-ir"
        ],
        "server": "dns-direct"
      }
    ],
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "tun",
      "inet4_address": "172.19.0.1/30",
      "auto_route": true,
      "inet4_route_exclude_address": [
        "10.0.0.0/8",
        "100.64.0.0/10",
        "169.254.0.0/16",
        "172.16.0.0/12",
        "192.0.0.0/24",
        "192.168.0.0/16",
        "224.0.0.0/24",
        "240.0.0.0/4",
        "239.255.255.250/32",
        "255.255.255.255/32"
      ],
      "inet6_route_exclude_address": [
        "fe80::/10",
        "fd00::/8",
        "ff01::/16",
        "ff02::/16",
        "ff03::/16",
        "ff04::/16",
        "ff05::/16",
        "fc00::/7"
      ],
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "Proxy",
      "outbounds": [
        "Auto",
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["CDN2"]},
        {"action":"include","keywords":["Austria|Sweden|Belgium|Romania|Netherlands|Turkey|Germany|Canada|Italy|Norway"]}
      ]
    },
    {
      "type": "urltest",
      "tag": "Auto",
      "outbounds": [
        "{all}"
      ],
      "url": "http://gstatic.com/generate_204",
      "interval": "10m0s",
      "filter":[
        {"action":"include","keywords":["CDN2"]},
        {"action":"include","keywords":["Austria|Sweden|Belgium|Romania|Netherlands|Turkey|Germany|Canada|Italy|Norway"]}
      ]
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "ip_is_private": true,
        "rule_set": [
          "geoip-ir",
          "geosite-ir"
        ],
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geoip-ir",
        "format": "binary",
        "url": "https://github.com/Chocolate4U/Iran-sing-box-rules/raw/rule-set/geoip-ir.srs",
        "download_detour": "direct"
      },
      {
        "type": "remote",
        "tag": "geosite-ir",
        "format": "binary",
        "url": "https://github.com/Chocolate4U/Iran-sing-box-rules/raw/rule-set/geosite-ir.srs",
        "download_detour": "direct"
      }
    ],
    "final": "Proxy",
    "auto_detect_interface": true,
    "override_android_vpn": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    }
  }
}
